#include <bits/stdc++.h>
using namespace std;
//s->smallest and ss-> second smallest

int secondSmallestElement(int arr[], int n){
    int s = arr[0], ss = -1;
    for(auto i = 1; i < n; i++){
        if(arr[i] < s){
            ss = s;
            s = arr[i];
        }
        if(arr[i] < ss && arr[i] > s){
            ss = arr[i];
        }
    }
    return ss;
}

int main(){
    int n;
    cin >> n;

    int arr[n];
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }

    cout << secondSmallestElement(arr, n);
    return 0;
}
