#include<bits/stdc++.h>
using namespace std;
 bool isSorted(int arr[],int n){
//no need to check after n-2
        for(auto i=0;i<n-2;i++){
            if(arr[i]>arr[i+1])
            return false;
        }
        return true;
 }    
        
int main() {
 int a[5]={11,11,22,33,44};
 cout<<isSorted(a,5);
  return 0;
}
