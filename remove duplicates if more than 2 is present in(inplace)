#include <bits/stdc++.h>
using namespace std;

int removeDup(vector<int>& arr) {
    int i = 1, count = 1;
    for (auto j = 1; j < arr.size(); j++) {
        if (arr[j] == arr[j - 1]) {
            count++;
        } else {
            count = 1;
        }
        if (count <= 2) {
            arr[i++] = arr[j];
        }
    }
    return i;
}

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int newLength = removeDup(arr);

    for (int i = 0; i < newLength; i++) {
        cout << arr[i] << " ";
    }

    return 0;
}
