#include <bits/stdc++.h>
using namespace std;

class solution {
public:
    void sumOfDiagonals(int matrix[][1000], int n) {
        //Write your code here...
        int pd=0,sd=0,k=0;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(i==j){
                    pd+=matrix[i][j];
                }
                if(j==(n-k-1) && i==k){
                    sd+=matrix[i][j];
                    k++;
                }
        }
        }
        cout << pd <<" "<<sd<<endl;
        
    }
};
