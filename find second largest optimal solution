#include <bits/stdc++.h>
using namespace std;

int secondLargestElement(int arr[], int n){
    int max = arr[0], sl = -1;
    for(auto i = 1; i < n; i++){
        if(arr[i] > max){
            sl = max;
            max = arr[i];
        }
        if(arr[i] > sl && arr[i] < max){
            sl = arr[i];
        }
    }
    return sl;
}

int main(){
    int n;
    cin >> n;

    int arr[n];
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }

    cout << secondLargestElement(arr, n);
    return 0;
}
