#include<bits/stdc++.h>
using namespace std;
 void movesZeoresFront(int a[],int n){
     int i=n-1,j;
     for(j=n-1;j>=0;j--){
         if(a[j]!=0){
             swap(a[j],a[i]);
             i--;
         }
     }
     for(int k=0;k<n;k++){
         cout<<a[k]<<" ";
     }
 }
int main() {
  int n;
  cout<<"enter n"<<endl;
  cin>>n;
  int a[n];
  cout<<"enter array elemnts"<<endl;
  for(int i=0;i<n;i++){
      cin>>a[i];
  }
  movesZeoresFront(a,n);
  return 0;
}
