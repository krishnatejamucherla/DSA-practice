class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int l=0;
        //moves zeroes to end order shoud be followed EX:[1,0,12,0,3]-->sol:[1,12,3,0,0]
        //use vector starts from 0 to end if we use from 1 to end the test case 1,0,0,1 fails
        for(int r=0;r<nums.size();r++){
            if(nums[r]!=0){
            swap(nums[l],nums[r]);
            l++;
            }
        }
        }
};
