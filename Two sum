#include<bits/stdc++.h>
using namespace std;
vector<int> tsum(vector<int>&nums,int n,int target){
    int i=0,j=n-1;
    while(i<j){
        int sum=nums[i]+nums[j];
        if(sum==target){
            return {i,j};
        }
        else if(sum>target){
            j--;
        }
        else
            i++;  
    }
    return {-1,-1};
}
int main(){
    int n,target;
    cin>>n;
    vector<int>nums(n);
    for(auto &it:nums){
        cin>>it;
    }
    cin>>target;
    vector<int>res=tsum(nums,n,target);
    if(res[0]==-1){
        cout<<"No such elements found"<<endl;
    }
    else{
        cout<<"Indices are: "<<res[0]<<" "<<res[1]<<endl;
    }
    return 0;
}
